[application]

config/name="Tank Battle"
config/description="เกมรถถังผจญภัยไปพร้อมกับอุปสรรค์มากมาย และยังสามารถต่อสู้เพื่อแย่งชิงทรัพยากรจากผู้เล่นอื่นให้ตัวเองเป็นคนเหลือรอดคนสุดท้ายและชนะเกมไป"
run/main_scene="res://src/main/title/StartScreen.tscn"
run/low_processor_mode=true
boot_splash/image="res://assets/ui/tank-edit/tank-edit/tanklogo6-54.png"
boot_splash/fullsize=false
boot_splash/bg_color=Color( 1, 0.654902, 0, 1 )
config/icon="res://assets/ui/tank-edit/tank-edit/tanklogo6-54.png"
run/main_scene.standalone="res://src/main/title/StartScreen.tscn"
run/main_scene.HTML5="res://src/main/title/StartScreen.tscn"
environment/default_environment="res://default_env.tres"

[autoload]

Nakama="*res://addons/com.heroiclabs.nakama/Nakama.gd"
OnlineMatch="*res://addons/nakama-webrtc/OnlineMatch.gd"
AdventureMatch="*res://src/main/adventure/autoload/AdventureMatch.gd"
Build="*res://src/autoload/Build.gd"
Globals="*res://src/autoload/Globals.tscn"
